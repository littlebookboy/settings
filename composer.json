{
  "name": "littlebookboy/settings",
  "keywords": ["settings", "persistent settings", "laravel"],
  "description": "Persistent settings package for Laravel framework.",
  "authors": [
    {
      "name": "Edvinas Kručas",
      "email": "edv.krucas@gmail.com"
    },
    {
      "name": "Gene Su",
      "email": "kevin2pig@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php" : ">=7.0",
    "illuminate/support": "~5.6",
    "illuminate/console": "~5.6",
    "illuminate/database": "^5.6",
    "illuminate/filesystem": "^5.2"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.3@dev"
  },
  "autoload": {
    "psr-0": {
      "Krucas\\Settings\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Krucas\\Settings\\Providers\\SettingsServiceProvider"
      ],
      "aliases": {
        "Settings": "Krucas\\Settings\\Facades\\Settings"
      }
    }
  }
}
